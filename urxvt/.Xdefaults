!
! ~/.Xdefaults
!

! Xcursor --------------------------------------------------------------------

!Xcursor*theme: insertthemehere
!Xcursor*size:  10

! Urxvt --------------------------------------------------------------------

urxvt*depth: 32
urxvt*background: rgba:0000/0000/0200/ffff

URxvt*geometry: 95x30
urxvt.foreground: #ffffff
!urxvt.font: xft:Dejavu Sans Mono:pixelsize=13:antialias=true
urxvt.font: xft:Source Code Pro:size=10:antialias=true
urxvt.modifier:alt
urxvt.urlLauncher: google-chrome
urxvt.matcher.button: 1
urxvt.perl-ext-common: default,matcher,tabbed
!urxvt.perl-ext-common: default,matcher,tabbedex

 urxvt.urgentOnBell: true
! Taille du buffer et barre de d√©filement
URxvt*saveLines: 10000
URxvt*scrollBar: true
URxvt*scrollBar_right: true
!URxvt*scrollColor: #c2dd5a
URxvt*scrollColor: #7ba2d2
URxvt*scrollBar_floating: true
URxvt*scrollstyle: plain

! Colors
urxvt.color0: #000000
urxvt.color1: #a40000
urxvt.color2: #4a9a06
urxvt.color3: #c4a000
urxvt.color4: #204a87
urxvt.color5: #5c3566
urxvt.color6: #729fcf
urxvt.color7: #eeeeec
urxvt.color8: #000000
urxvt.color9: #cc0000
urxvt.color10: #73d216
urxvt.color11: #edd409
urxvt.color12: #3465a4
urxvt.color13: #75507b
urxvt.color14: #729fcf
urxvt.color15: #ffffff

! do not scroll with output
URxvt*scrollTtyOutput: false

! ! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! ! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true


!-----------------
!tabs colors
!----------------
!URxvt*tabbed*tabbar-fg: 8
!URxvt*tabbed*tabbar-bg: 0
!URxvt*tabbed*tab-fg: 8
!URxvt*tabbed*tab-bg: 1

!----------------
!Clipboard
!----------------
URxvt*keysym*M-c: perl:clipboard:copy
URxvt*keysym*C-v: perl:clipboard:paste


! Xterm --------------------------------------------------------------------

! General
xterm*background: #242424
xterm*foreground: #ffffff
xterm*faceName: xft:Dejavu Sans Mono:pixelsiz
e=13:antialias=true

! Colors
xterm*color0: #000000
xterm*color1: #a40000
xterm*color2: #4a9a06
xterm*color3: #c4a000
xterm*color4: #204a87
xterm*color5: #5c3566
xterm*color6: #729fcf
xterm*color7: #eeeeec
xterm*color8: #000000
xterm*color9: #cc0000
xterm*color10: #73d216
xterm*color11: #edd409
xterm*color12: #3465a4
xterm*color13: #75507b
xterm*color14: #729fcf
xterm*color15: #ffffff

!! get option over to work
URxvt.keysym.M-Left:        \033[1;5D
URxvt.keysym.M-Right:       \033[1;5C
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C

! Xcalc --------------------------------------------------------------------

XCalc*ti.backgroundPixmap: gradient:vertical?dimension=500&start=gray90&end=gray10

